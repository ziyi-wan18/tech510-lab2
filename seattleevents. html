    $(function() {
        /* == Initialization == */

        var initFilterMenu = function() {
            var $currentPageHeader = $(".filter-header.current-page");

            // Set 'frm'
            if ($currentPageHeader.length) {
                setOriginFilter($currentPageHeader);
            }

            // Set refine button state
            if ($('.filter-content').hasClass('visible')) {
                emphasizeButton();
            }
        };

        var initDatepickers = function() {
            var $startDatePicker = $('.event-date-picker.event-start'),
                $startDateHidden = $('#event_begin'),
                $endDatePicker = $('.event-date-picker.event-end'),
                $endDateHidden = $('#event_end'),
                currentDate;

            // Set start date to today
            if (isNaN(parseInt($startDatePicker.val()))) {
                currentDate = moment();
                $startDatePicker.val(currentDate.format('MM/DD/YYYY'));
                $startDateHidden.val(currentDate.format('MM/DD/YYYY'));
            }
            // Set end date to 'End Date' (don't constrain)
            if (typeof($endDatePicker.val()) === 'undefined' || $endDatePicker.val() === '') {
                $endDatePicker.val('End Date');
                $endDateHidden.val('');
            }

            // Refine button state
            toggleEmphasis();
        };

        var buildPartnerFilters = function() {
            var $searchFilterType = $(".js-searchFilterType"),
                $searchFiltersCat = $(".js-searchFilterParentCats"),
                $searchFiltersSubCat = $(".js-searchFilterChildCats"),
                selectedTypeIndex = 0,
                selectedCatIndex = '',
                selectedSubCats = [],
                search = location.search.match(/partner_cat(?:\[]|%5[bB]%5[dD])?=([a-zA-Z\-]*)/g);

            var populateTypeList = function(types) {
                var typeFilter = '<option value="">- Refine by Listing Source</option>',
                    selectedType = '';
                $.each(types, function(i, val) {
                    typeFilter += '<option ' + (selectedType === val.value ? 'selected' : '')
                        + ' value="' + val.value + '">' + val.name
                        + '</option>';
                    if (selectedType === val.value) {
                        selectedTypeIndex = i;
                    }
                });
                return typeFilter;
            },
            populateCategoryList = function(categories, shouldSelect) {
                var catFilter = '<option value="">- Refine by Business Category</option>',
                    selectedCat = '';
                $.each(categories, function(i, val) {
                    catFilter += '<option ' + (shouldSelect && selectedCat === val.value ? 'selected' : '')
                        + ' value="' + val.value + '">' + val.name
                        + '</option>';
                    if (selectedCat === val.value) {
                        selectedCatIndex = i;
                    }
                });
                return catFilter;
            },
            populateSubCategoryList = function(subCategories, shouldSelect) {
                var subCatFilter = '';
                // List of subcategories fetched once, outside function
                $.each(subCategories, function(i, val) {
                    subCatFilter += '<div class="filter-option">';
                    subCatFilter += '<input type="checkbox" id="partner-filter-child-cat_' + i + '" name="partner_cat[]"'
                        + (shouldSelect && selectedSubCats.indexOf(val.value) > -1 ? 'checked' : '')
                        + ' value="' + val.value + '">';
                    subCatFilter += '<label for="partner-filter-child-cat_' + i + '" class="checkbox">' + val.title + '</label>';
                    subCatFilter += '</div>';
                });
                return subCatFilter;
            };

            // Get selected categories from URL
            $.each(search, function(i, val) {
                selectedSubCats.push(val.split('=')[1]);
            });

            /* === Create filters === */

            // Partner types
            $searchFilterType.html(populateTypeList(partnerFilters));

            // Partner categories
            $searchFiltersCat.html(populateCategoryList(partnerFilters[selectedTypeIndex]['parentcats'], true));

            // Partner subcategories
            if (!isNaN(parseInt(selectedCatIndex))) {
                $searchFiltersSubCat.html(
                    populateSubCategoryList(partnerFilters[selectedTypeIndex]['parentcats'][selectedCatIndex]['childcats'], true)
                );
            }

            /* === Handle filter change events === */

            // Partner source/type change
            $searchFilterType.on("change", function() {
                // Re-populate categories
                $searchFiltersCat.children().remove();
                for (var i = 0; i < partnerFilters.length; i++) {
                    if (partnerFilters[i].value === this.value) {
                        selectedTypeIndex = i;
                        $searchFiltersCat.html(populateCategoryList(partnerFilters[selectedTypeIndex]['parentcats']), false);
                    }
                }
                // Display subcategory message
                $searchFiltersSubCat.children().remove();
                $searchFiltersSubCat.html('<span class="filter-message">Select a business category first</span>');
            });

            // Partner parent category change
            $searchFiltersCat.on("change", function() {
                var subCatFilter = '';
                // Re-populate subcatgories
                $searchFiltersSubCat.children().remove();
                for (var i = 0; i < partnerFilters[selectedTypeIndex]['parentcats'].length; i++) {
                    if (partnerFilters[selectedTypeIndex]['parentcats'][i].value === this.value) {
                        subCatFilter = populateSubCategoryList(partnerFilters[selectedTypeIndex]['parentcats'][i]['childcats'], false);
                        break;
                    }
                }
                if (subCatFilter === '') {
                    subCatFilter = '<span class="filter-message">No sub-categories</span>';
                }
                $searchFiltersSubCat.html(subCatFilter);
            });
        };

        /* === Event handling === */

        var registerListeners = function() {
            var MAX_PHONE_WIDTH = 640,
                $filterBlock = $('.filter-results');

            // Main search form keypress - submit on enter
            $('.search-form-query-input').keypress(function(e) {
                if (e.which === 13) {
                    submitSearch(e);
                }
            });

            // Window resize - toggle mobile layout
            $(window).on('resize', function() {
                var isShown = $filterBlock.hasClass('show-for-mobile');
                if ((window.innerWidth > MAX_PHONE_WIDTH) && isShown) {
                    hideMobileFilter($filterBlock);
                }
            });

            $(document)
                // Expandable menu CLICK - toggle expandable menu
                .on('click', '.filter-header.expandable', function(e) {
                    var $contentBlock = $(this).next('.filter-content'),
                        isExpanded = $contentBlock.hasClass('visible');
                    $('.filter-content').removeClass('visible');
                    $('.filter-header').removeClass('selected');
                    if (!isExpanded) {
                        $contentBlock.addClass('visible');
                        $(this).addClass('selected');
                        setOriginFilter($(e.currentTarget));
                    }
                })
                // Mobile filter button CLICK - show menu
                .on('click', '.filter-btn', function() {
                    var isShown = $filterBlock.hasClass('show-for-mobile');
                    if (!isShown) {
                        showMobileFilter($filterBlock);
                    }
                })
                // Mobile close buttons CLICK - hide menu
                .on('click', '.filter-close-btn, .mobile-close-button', function() {
                    var isShown = $filterBlock.hasClass('show-for-mobile');
                    if (isShown) {
                        hideMobileFilter($filterBlock);
                    }
                })
                // Start date CHANGE - constrain end date
                .on('change', '.event-date-picker.event-start', function(e) {
                    var newStartDate = moment(e.currentTarget.value, 'MM/DD/YYYY'),
                        $endDatePicker = $('.event-date-picker.event-end'),
                        endDate = moment($endDatePicker.val(), 'MM/DD/YYYY');
                    if (newStartDate.isAfter(endDate)) {
                        var newEndDate = newStartDate.clone().add(1, 'days');
                        $endDatePicker.datepicker('setDate', newEndDate.format('MM/DD/YYYY'));
                    }
                })
                // End date CHANGE - constrain start date
                .on('change', '.event-date-picker.event-end', function(e) {
                    var newEndDate = moment(e.currentTarget.value, 'MM/DD/YYYY'),
                        $startDatePicker = $('.event-date-picker.event-start'),
                        startDate = moment($startDatePicker.val(), 'MM/DD/YYYY');
                    if (newEndDate.isBefore(startDate)) {
                        var newStartDate = newEndDate.clone().subtract(1, 'days');
                        $startDatePicker.datepicker('setDate', newStartDate.format('MM/DD/YYYY'));
                    }
                })
                // Submit buttons CLICK - submit form
                .on('click', '#js-btn-submit, .search_submit input[type="submit"]', function(e) {
                    submitSearch(e);
                })
                // Menu header CLICK - change clear/submit button states
                .on('click', '.filter-header', function(e) {
                    toggleEmphasis(e);
                })
                // Menu state CHANGE - change clear/submit button states
                .on('change', '.filter-results-wrapper', function(e) {
                    toggleEmphasis(e);
                })
                // Menu link CLICK - submit search for type
                .on('click', '.filter-header.expandable a', function(e) {
                    setSearchType(e);
                    submitSearch(e);
                })
                // Filter link CLICK - submit search for page type
                .on('click', '.is-link', function(e) {
                    // Clear sub-fitlers
                    if ($(e.target).hasClass('js-filter_all') || $(e.target).parents('li').hasClass('js-filter_all')) {
                        clearFilters();
                    }
                    setSearchType(e);
                    submitSearch(e);
                })
                // Clear button CLICK - clear filters
                .on('click', '#js-btn-clear', function(e) {
                    e.preventDefault();
                    clearFilters();
                    $('.filter-header').removeClass('selected');
                });

        };

        var hideMobileFilter = function($filterBlock) {
            $filterBlock.removeClass('show-for-mobile');
            $(window).off('scroll.menuOpen')
        };

        var showMobileFilter = function($filterBlock) {
            $filterBlock.addClass('show-for-mobile');
            $(window).on('scroll.menuOpen', debounce(function() { moveMobileCloseBtn(); }, 50));
        };

        var moveMobileCloseBtn = function() {
            var siteHeaderRect = document.querySelector('.fixed-header').getBoundingClientRect(),
                filterHeaderRect = document.querySelector('.filter-results-header').getBoundingClientRect(),
                closeContainer = document.querySelector('.mobile-close-container'),
                filterLinksRect = document.querySelector('.filter-links').getBoundingClientRect();


            if ((filterHeaderRect.top + filterHeaderRect.height) > (siteHeaderRect.top + siteHeaderRect.height)) {
                // Filter header in view - Hide close button
                closeContainer.classList.remove('fix-to-screen');
                closeContainer.classList.remove('fix-to-container');
            } else {
                // Filter header out of view - Show close button button
                if ((filterLinksRect.top + filterLinksRect.height) < window.innerHeight) {
                    // Filter link block bottom in view - Fix
                    closeContainer.classList.remove('fix-to-screen');
                    closeContainer.classList.add('fix-to-container');
                } else {
                    // Filter link block bottom out of view - Float
                    closeContainer.classList.remove('fix-to-container');
                    closeContainer.classList.add('fix-to-screen');
                }
            }
        };

        var submitSearch = function(e) {
            e.preventDefault();
            $('#searchform').submit();
        };

        var setSearchType = function(e) {
            var $target = (e.target.tagName === 'A') ? $(e.target) : $(e.target).find('a'),
                $fromFilter = $('#filter-frm'),
                searchType = $target.attr('href').match(/frm=(.*)&?/)[1];
            $fromFilter.val(searchType);
        };

        var toggleEmphasis = function(e) {
            var filterValues = $('#searchform').serializeArray(),
                defaultKeys = {
                    's': '',
                    'event_begin': moment().format('MM/DD/YYYY'),
                    'event_end': '',
                    'partner_parent_cat': ''
                },
                event = e || null;

            if (event && ($(event.currentTarget).hasClass('selected')
                    || $(event.target).parents('.filter-content').prev().hasClass('selected'))) {
                // Filter drawer is open => enable
                emphasizeButton();
            } else {
                // Check the filters for changes
                for (var i = 0; i < filterValues.length; i++) {
                    if (filterValues[i].name === 'frm') {
                        // Ignore this token, it's controlled by drawer state
                        // Also there's a race condition regarding setting the value
                    } else if (Object.keys(defaultKeys).indexOf(filterValues[i].name) === -1) {
                        // New filter was added => enable
                        emphasizeButton();
                        break;
                    } else if (filterValues[i].value !== defaultKeys[filterValues[i].name]) {
                        // Filter value changed => enable
                        emphasizeButton();
                        break;
                    } else {
                        // Back to cleared state => disable
                        muteButton();
                    }
                }
            }
        };

        /* == Utility functions == */

        var setOriginFilter = function($target) {
            var searchOrigin = $target.attr('class').match(/js-filter_(\w*)\s/)[1];
            if (searchOrigin) {
                $('#filter-frm').val(searchOrigin);
            }
        };

        var clearFilters = function() {
            var $filterDrawers = $('.filter-content'),
                $filterCheckboxes = $('.filter-results-wrapper input[type="checkbox"]'),
                $filterDropdowns = $('.filter-results-wrapper select'),
                $eventDatePicker = $('.event-date-picker'),
                $searchTypeFilter = $('#filter-frm'),
                $childCatFilter = $('.js-searchFilterChildCats');

            // Set refine button state to muted
            muteButton();

            // Close all filter drawers
            $filterDrawers.each(function() {
                $(this).removeClass('visible');
            });

            // Clear checkboxes
            $filterCheckboxes.each(function(i, elem) {
                elem.checked = false;
            });

            // Clear dropdowns
            $filterDropdowns.each(function(i, elem) {
                $(elem).find("option:first").attr("selected", "selected");
            });

            // Except set ptype to visitor's guide
            $('.js-searchFilterType').find("option:nth-of-type(2)").attr("selected", "selected");

            // Clear date fields
            $eventDatePicker.datepicker('setDate', '');
            initDatepickers();

            // Remove child category selection
            $childCatFilter.empty();
            $childCatFilter.html('<span class="filter-message">Select a business category first</span>');

            // Reset top level filter
            $searchTypeFilter.val('all');
        };

        var emphasizeButton = function() {
            var $refineBtn = $('.filter-results-footer button[type="submit"]'),
                $mobileRefineBtn = $('.show-for-small .filter-btn');
            $refineBtn.removeClass('muted');
            $mobileRefineBtn.removeClass('muted');
            $refineBtn.addClass('emphasized');
            $mobileRefineBtn.addClass('emphasized');
        };

        var muteButton = function() {
            var $refineBtn = $('.filter-results-footer button[type="submit"]'),
                $mobileRefineBtn = $('.show-for-small .filter-btn');
            $refineBtn.removeClass('emphasized');
            $mobileRefineBtn.removeClass('emphasized');
            $refineBtn.addClass('muted');
            $mobileRefineBtn.addClass('muted');
        };

        var debounce = function (func, wait, immediate) {
            // @see https://davidwalsh.name/javascript-debounce-function
            var timeout;
            return function () {
                var context = this, args = arguments;
                var later = function () {
                    timeout = null;
                    if (!immediate) func.apply(context, args);
                };
                var callNow = immediate && !timeout;
                clearTimeout(timeout);
                timeout = setTimeout(later, wait);
                if (callNow) func.apply(context, args);
            };
        };

        /* Initialization function */

        var init = function() {
            initFilterMenu();
            initDatepickers();
            buildPartnerFilters();
            registerListeners();
        };
        init();
    });
});
</script>
