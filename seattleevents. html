<script  id="vs-event_filters">
    (function ($) {
        var startDate = $('.event-start'),
            endDate = $('.event-end');

        if (typeof initDatepicker !== 'function') {
            function initDatepicker() {
                startDate.datepicker({
                    altField: '#event_begin',
                    format: 'yy-mm-dd'
                });

                endDate.datepicker({
                    altField: '#event_end',
                    format: 'yy-mm-dd'
                });
            }
        }

        if (!startDate.hasClass('hasDatepicker')) {
            $(document).ready(function() {
                initDatepicker();
            })
        }
    }(jQuery));
</script>
