                    unit: '/1020224/visit-seattle-leaderboard',
                    size: [728, 90],
                    id: 'div-gpt-ad-1429570561298-2'
                },
                {
                    unit: '/1020224/visit-seattle-skyscraper',
                    size: [160, 600],
                    id: 'div-gpt-ad-1429570561298-3'
                },
