                    unit: '/1020224/visit-seattle-skyscraper-2',
                    size: [160, 600],
                    id: 'div-gpt-ad-1429570561298-4'
                }
            ];

            googletag.cmd.push(function () {
                ads.forEach(function (ad) {
                    if ($('#' + ad.id).length > 0) {
                        googletag.defineSlot(ad.unit, ad.size, ad.id).addService(googletag.pubads());
                    }
                });
            });

            googletag.pubads().enableSingleRequest();
            googletag.enableServices();
        });
    </script>
